{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/favicon.ico",
      "/images/*",
      "/vite.svg"
    ]
  },
  "routes": [
    { "route": "/assets/*", "headers": { "cache-control": "public, max-age=31536000, immutable" } }
  ],
  "mimeTypes": {
    ".json": "application/json"
  }
}
